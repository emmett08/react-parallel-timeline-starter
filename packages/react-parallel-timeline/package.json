{
  "name": "@neuralsea/react-parallel-timeline",
  "version": "0.1.2",
  "type": "module",
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles.css": "./dist/styles.css"
  },
  "peerDependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "shx": "^0.3.4",
    "tsup": "^8.0.0",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "tsup && shx cp src/styles.css dist/styles.css",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  }
}
